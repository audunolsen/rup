{
  "name": "sheepit",
  "version": "0.0.1",
  "author": "Audun Meek Olsen",
  "repository": {
    "type": "git",
    "url": "https://github.com/audunolsen/sheepit.git"
  },
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/",
    "./dist/internals/*": null
  },
  "scripts": {
    "swc": "swc src ./src --out-dir ./dist --source-maps",
    "compile": "concurrently --raw 'npm run swc' 'tsc'",
    "build": "rimraf ./dist && npm run compile --silent"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
